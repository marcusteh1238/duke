<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" fillHeight="false" maxHeight="1.7976931348623157E308" prefWidth="400.0" spacing="10.0" style="-fx-padding: 10; -fx-border-color: black;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" maxHeight="1.7976931348623157E308" style="-fx-padding: 10; -fx-background-color: #0e18dd;" text="Label" textFill="WHITE" wrapText="true" HBox.hgrow="ALWAYS">
         <font>
            <Font name="Eras Demi ITC" size="12.0" />
         </font></Label>
      <Circle fx:id="myCircle" fill="DODGERBLUE" radius="50.0" stroke="#0e18dd00" strokeType="INSIDE" style="-fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.8), 10, 0, 0, 0);" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </Circle>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
